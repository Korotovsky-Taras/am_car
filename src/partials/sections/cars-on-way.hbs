            </swiper-slide>
            <swiper-slide>
                {{> components/stock-thumb }}
            </swiper-slide>
        </swiper-container>
        <div class="app-section--footer app-section--footer--mt app-mobile-visible">
            <a href="#" class="app-btn btn-size-m btn-secondary btn-full">
                <span>Все авто в пути</span>
                <svg width="9" height="24">
                    <use xlink:href="/app-sprite.svg#arrow-right"></use>
                </svg>
            </a>
        </div>
